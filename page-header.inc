<!doctype html>
<html ${htmlAttr} class="no-js">
	<!-- Generated by jAlbum: https://jalbum.net at ${currentDate} (<ja:if test="${license}" not value="">${license}</ja:if><ja:else>unregistered</ja:else>) -->
	<head>
		<meta charset="${textEncoding}">
		<meta http-equiv="x-ua-compatible" content="ie=edge">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<title>${pageTitle}<ja:if exists="titleSEOText"> ${titleSEOText}</ja:if></title>
		<ja:if exists="headlineFont"><link rel="stylesheet" href="https://fonts.googleapis.com/css?family=<%= getGoogleFont(headlineFont) %>"></ja:if>
		<meta name="description" content="<ja:if exists="pageDescription">${pageDescription}</ja:if><ja:if exists="descriptionSEOText"> ${descriptionSEOText}</ja:if>">
		<ja:if exists="writer"><meta name="author" content="${writer}"></ja:if>
		<meta name="generator" content="jAlbum ${internalVersion} & ${skinName} ${skinVersion} [${styleName}]">
		<ja:if test=<%= typeof keywords !== 'undefined' || typeof albumTags !== 'undefined' %>><meta name="keywords" content="${keywords} ${albumTags}"></ja:if> 
		<ja:if exists="pageUrl"><meta property="og:url" content="${pageUrl}"></ja:if>
		<ja:if exists="pageThumbPath"><meta property="og:image" content="${pageThumbPath}">
		<meta property="og:image:width" content="600">
		<meta property="og:image:height" content="420">
		<link rel="image_src" href="${pageThumbPath}">
		<meta name="twitter:image" content="${pageThumbPath}"></ja:if>
		<meta property="og:title" content="${pageTitle}">
		<meta property="og:description" content="${pageDescription}">
		<meta property="og:type" content="website">
		<ja:if exists="facebookAppId"><meta property="fb:app_id" content="${facebookAppId}"></ja:if>
		<meta name="twitter:title" content="${pageTitle}">
		<meta name="twitter:card" content="summary">
		<meta name="twitter:description" content="${pageDescription}">
		<meta name="apple-mobile-web-app-status-bar-style" content="black-translucent">
		<meta name="apple-mobile-web-app-capable" content="yes">
		<meta name="format-detection" content="telephone=no">
		<link rel="stylesheet" href="${resPath}/common.css?v=${skinVersion}">
		<%= getCustomPageCSS(currentObject); %>
		<ja:if test="${useFavicon}"><link rel="apple-touch-icon-precomposed" href="${resPath}/apple-touch-icon-144x144-precomposed.png">
		<link rel="icon" type="image/png" href="${resPath}/favicon-32x32.png" sizes="32x32">
		<link rel="icon" type="image/png" href="${resPath}/favicon-192x192.png" sizes="192x192">
		<!--[if IE]><link rel="shortcut icon" href="${resPath}/favicon.ico"><![endif]-->
		<meta name="msapplication-TileColor" content="#004b85">
		<meta name="msapplication-TileImage" content="${resPath}/tileicon.png"></ja:if>
		<meta name="theme-color" content="<%= getCssColor(topmenuColor) %>">
		<!--[if (lte IE 9)]><script src="${resPath}/modernizr.js"></script><![endif]-->
		<ja:if exists="headHook"><%= getProcessed(headHook) %></ja:if>
		<ja:if test="${googleAnalytics}" value="gtag">
		<script async src="https://www.googletagmanager.com/gtag/js?id=${googleSiteID}"></script>
		<script>
			window.dataLayer=window.dataLayer||[];
			function gtag(){dataLayer.push(arguments);}
			gtag('js',new Date());
			gtag('config','${googleSiteID}',{'send_page_view':false});
		</script>
		</ja:if>
	</head>
	<body id="${pageType}" class="<%= pageType + ' ' + (level? 'sub-album' : 'top-level') + ((pageType === 'page')? (' ' + label.toLowerCase()) : '')  %>">
		<%-- Above body custom code --%>
		<ja:if test=<%= bodyTopHook && !(bodyTopTopLevelOnly && level) %>>
		<ja:if test="bodyTopFitWidth">
		<div class="row above-body">
			<div class="columns">
				<%= getProcessed(bodyTopHook) %>
			</div>
		</div>
		</ja:if>
		<ja:else>
		<%= getProcessed(bodyTopHook) %>
		</ja:else>
		</ja:if>
		
		<%-- Top bar --%>
		<ja:if test=<%= logoName || topNavigation || useSearch || backgroundMusic %>>
		<div class="page-top">
			<div class="row">
				<div class="large-12 columns">
					<div class="title-bar hide-for-medium" data-responsive-toggle="responsive-menu" data-hide-for="medium">
						<ja:if exists="topNavigation">
						<button class="icon-menu" type="button" data-toggle="responsive-menu" data-tooltip title="${text.folders}"></button>
						</ja:if>
						<ja:if exists="logoName"><div class="logo"><%-- Logo --%>
						<ja:if test=<%= level > 0 || pageType === 'page' %>><a href="${rootPath}/${indexName}" data-tooltip title="${text.home}"><img src="${resPath}/<%= encodeURIComponent(logoName) %>" alt="logo"></a></ja:if>
						<ja:else><ja:if exists="homepageAddress"><a href="${homepageAddress}"<ja:if exists="homepageLinkText"> data-tooltip title="${homepageLinkText}"</ja:if>><img src="${resPath}/<%= encodeURIComponent(logoName) %>" alt="logo"></a></ja:if>
						<ja:else><img src="${resPath}/<%= encodeURIComponent(logoName) %>" alt="logo"></ja:else></ja:else>
						</div></ja:if>
					</div>
					<div class="top-bar<ja:if exists="logoName"> has-logo</ja:if><ja:if exists="topNavigation"> has-menu</ja:if><ja:if test="${useSearch}"> has-search</ja:if>" id="responsive-menu">
						<div class="top-bar-left">
							<ja:if exists="logoName"><%-- Logo --%>
							<div class="logo show-for-medium"><ja:if test=<%= level > 0 || pageType === 'page' %>><a href="${rootPath}/${indexName}" data-tooltip title="${text.home}"><img src="${resPath}/<%= encodeURIComponent(logoName) %>" alt="logo"></a></ja:if>
							<ja:else><ja:if exists="homepageAddress"><a href="${homepageAddress}"<ja:if exists="homepageLinkText"> data-tooltip title="${homepageLinkText}"</ja:if>><img src="${resPath}/<%= encodeURIComponent(logoName) %>" alt="logo"></a></ja:if>
							<ja:else><img src="${resPath}/<%= encodeURIComponent(logoName) %>" alt="logo"></ja:else></ja:else>
							</div></ja:if>
							<ja:if exists="topNavigation"><%-- Top navigation --%>
							${topNavigation}
							</ja:if>
						</div>
						<div class="top-bar-right">
							<%-- Search --%>
							<ja:if test=<%= _useSearch && pageType !== 'page' %>>
							<form class="search <ja:if test="${searchVisibleByDefault}">active</ja:if><ja:else>closed</ja:else>">
								<div class="search-input"><input type="text" placeholder="${text.searchBoxTip}" aria-label="${text.search}"></div>
								<a class="search-btn icon-search" data-tooltip title="${text.search}"></a>
							</form>
							</ja:if>
							<%-- Background audio --%>
							<ja:if exists="backgroundAudio">
							<%
								out.println(
									'<a class="player-btn btn" data-rel="playlist" data-audioplayer=\'' + JSON.stringify(jsAudioPlayerVars) + '\'>' +
									// Pause button
									'<svg class="pause-btn" width="20px" height="20px" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg">' +
									'<g class="disc" fill="' + topmenuTextColor + '"><path d="M3.812,16.188C2.229,14.603,1.25,12.416,1.25,10s0.979-4.603,2.562-6.188L2.929,2.929C1.12,4.738,0,7.238,0,10s1.12,5.262,2.929,7.071L3.812,16.188z"></path>	<path d="M16.188,3.812C17.771,5.397,18.75,7.584,18.75,10s-0.979,4.603-2.562,6.188l0.883,0.883C18.88,15.262,20,12.762,20,10s-1.12-5.262-2.929-7.071L16.188,3.812z"></path></g>' +
									'<g class="pause-icon" fill="' + topmenuTextColor + '"><rect x="6" y="6" width="3" height="8"></rect><rect x="11" y="6" width="3" height="8"></rect></g>' +
									'</svg>' +
									// Play button
									'<svg class="play-btn" width="20px" height="20px" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg">' +
									'<g class="play-icon" fill="' + topmenuTextColor + '"><path d="M10,0C4.477,0,0,4.477,0,10s4.477,10,10,10s10-4.477,10-10S15.523,0,10,0z M7,14.571V5.429L15,10L7,14.571z"></path></g>' +
									'</svg></a>'
								);
							%>
							</ja:if>
						</div>
					</div>
				</div>
			</div>
		</div>
		</ja:if>

		<%-- Hero --%>
		<div class="hero"<ja:if test="<%= heroFullWidth && heroType !== 'flatColor' %>"> style="background-image:url(<ja:if test="${heroType}" value="albumImage">${rootPath}/</ja:if>${folderImageFileName});"</ja:if>>
			<div class="row">
				<div class="large-12 columns">
					<div class="hero-image"<ja:if test="<%= !heroFullWidth && heroType !== 'flatColor' %>"> style="background-image:url(<ja:if test="${heroType}" value="albumImage">${rootPath}/</ja:if>${folderImageFileName});"</ja:if>>
						<div class="buttons"><%--
							--%><ja:if exists="header"><a class="button icon-info" data-modal-open="album-info" data-tooltip title="${text.aboutThisAlbum}"></a></ja:if><%--
							--%><ja:if test="${_anyShares}"><a class="button icon-connect" data-modal-open="social-share" data-tooltip title="${text.shareAlbum}"></a></ja:if><%--
							--%><ja:if exists="zipFile"><a href="${zipFile}" class="button icon-download" data-tooltip title="${text.downloadImages}" download></a></ja:if><%--
							--%><ja:if test="${showHelp}"><a class="button icon-question" data-modal-open="help" data-tooltip title="${text.help}"></a></ja:if><%--
						--%></div>
						<header>
							<ja:if test="${level}" not value="0"><a class="back-btn nav-link icon-backward-light" href="../${indexName}" <ja:if test="${showBreadcrumbPath}" value="none">data-tooltip title="${text.upOneLevel}"</ja:if><ja:else><ja:if test="${showBreadcrumbPath}" value="tooltip">data-tooltip="#breadcrumb"</ja:if></ja:else>></a></ja:if><%--
							--%><ja:else><ja:if exists="homepageAddress"><a class="back-btn nav-link icon-backward-light" href="${homepageAddress}" target="_parent" data-tooltip title="<ja:if exists="homepageLinkText">${homepageLinkText}</ja:if><ja:else>${text.home}</ja:else>"></a></ja:if><%--
							--%><ja:else><ja:if test="${pageType}" value="page"><a class="back-btn nav-link icon-backward-light" href="${indexName}"></a></ja:if></ja:else></ja:else><%--
							--%><div>${folderTitle}<%--
							--%><ja:if test="${showBreadcrumbPath}" not value="none"><div id="breadcrumb" class="breadcrumb<ja:if test="${showBreadcrumbPath}" value="tooltip"> hidden</ja:if>">${breadcrumbPath}</div></ja:if><%--
							--%></div>
						</header>
						<ja:if test=<%= pageType !== 'page' && showStartSlideshow %>><div class="startshow"><span class="text">${text.startSlideshow}</span><a class="large button icon-play"></a></div></ja:if>
					</div>
				</div>
			</div>
		</div>
		
		<%-- Main content --%>
		<div class="main" id="main"> 
			<div class="row">
				<div id="main-cont" class="columns">
					<article class="cont<ja:if test="${pageType}" value="page"> feedbox</ja:if>">